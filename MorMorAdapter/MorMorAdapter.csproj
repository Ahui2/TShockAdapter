<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="protobuf-net.Core.dll" />
    <None Remove="protobuf-net.dll" />
    <None Remove="SixLabors.ImageSharp.dll" />
    <None Remove="System.Collections.Immutable.dll" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="protobuf-net.Core.dll" />
    <EmbeddedResource Include="protobuf-net.dll" />
    <EmbeddedResource Include="SixLabors.ImageSharp.dll" />
    <EmbeddedResource Include="System.Collections.Immutable.dll" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="TShock" Version="5.2.0" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="protobuf-net">
      <HintPath>protobuf-net.dll</HintPath>
    </Reference>
    <Reference Include="protobuf-net.Core">
      <HintPath>protobuf-net.Core.dll</HintPath>
    </Reference>
    <Reference Include="SixLabors.ImageSharp">
      <HintPath>SixLabors.ImageSharp.dll</HintPath>
      <Private>False</Private>
    </Reference>
    <Reference Include="System.Collections.Immutable">
      <HintPath>System.Collections.Immutable.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
